	"license": "MIT",
	"description": "",
	"keywords": [],
	"scripts": {
		"start": "npm run dev",
		"dev": "npm-run-all -p watch:js serve",
		"serve": "live-server . --watch src --no-browser",
		"watch:js": "babel src/ --watch --out-dir lib/",
		"prebuild": "babel src -d lib",
		"build": "npm-run-all -l -s prebuild  \"snowpack --optimize\"",
		"snowpack": "snowpack",
		"postinstall": "npm run snowpack"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kyunwang/gerart.git"
	},
	"bugs": {
		"url": "https://github.com/kyunwang/gerart/issues"
	},
	"homepage": "https://github.com/kyunwang/gerart",
	"devDependencies": {
		"@babel/cli": "^7.8.0",
		"@babel/core": "^7.8.0",
		"@babel/preset-env": "^7.8.0",
		"live-server": "^1.2.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0",
		"sass": "^1.23.7",
		"serve": "^11.3.0",
		"snowpack": "^1.0.3"
	},
	"dependencies": {
		"csz": "^1.1.1",
		"p5": "^0.10.2",
		"preact": "^10.2.1"
	}
