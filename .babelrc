{
	"plugins": [
		["snowpack/assets/babel-plugin.js", {}],
		[
			"@babel/plugin-transform-react-jsx",
			{
				"pragma": "h", // default pragma is React.createElement
				"pragmaFrag": "Fragment", // default is React.Fragment
				"throwIfNamespace": false // defaults to true
			}
		]
	],
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"esmodules": true
				},
				"modules": "false",
				"useBuiltIns": false
			}
		]
	]
}
